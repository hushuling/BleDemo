<?xml version="1.0" encoding="utf-8"?>
        

  <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" android:tag="layout/activity_bluetooth_conect_0">
<LinearLayout
  android:id="@+id/line1"
  android:layout_width="match_parent"
  android:layout_height="wrap_content">
  <TextView
    android:id="@+id/tv_stuat"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:gravity="center"
    android:lineSpacingExtra="10px"
    android:text="" />
  <Button
    android:id="@+id/btn_stop"
    android:text="停止检测"
    android:layout_alignParentRight="true"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"/>
  <Button
    android:id="@+id/btn_restart"
    android:text="设备连接异常，重置"
    android:layout_alignParentRight="true"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"/>
</LinearLayout>
    <TextView
      android:id="@+id/tv_result"
      android:visibility="gone"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"/>

    <Button
      android:id="@+id/btn_qingniu"
      android:text="轻牛体脂秤"
      android:layout_alignParentRight="true"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"/>
    <Button
      android:id="@+id/btn_xueya"
      android:text="爱奥乐血压"
      android:layout_alignParentRight="true"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"/>
    <LinearLayout
      android:layout_width="wrap_content"
      android:layout_height="wrap_content">
      <Button
        android:id="@+id/btn_Kruikang_oxgen"
        android:text="科瑞康血氧"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
      <Button
        android:id="@+id/btn_Kangtai_oxgen"
        android:text="康泰血氧"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
      <com.xiekang.bluetooths.bluetooths.oxgen.draw.DrawThreadNW
        android:id="@+id/DrawThreadNW"
        android:layout_gravity="center"
        android:layout_width="587px"
        android:layout_height="170px"/>
    </LinearLayout>
<LinearLayout
  android:layout_width="wrap_content"
  android:layout_height="wrap_content">
  <Button
    android:id="@+id/btn_aikang"
    android:text="艾康血脂"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"/>
  <Button
    android:id="@+id/btn_Sugar_Oxygen"
    android:text="艾康血红蛋白"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"/>
  <Button
    android:id="@+id/btn_Kadik"
    android:text="卡迪克血脂"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"/>
  <Button
    android:id="@+id/btn_lepu"
    android:text="乐普血脂"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"/>
</LinearLayout>

    <LinearLayout
      android:layout_width="wrap_content"
      android:layout_height="wrap_content">
      <Button
        android:id="@+id/btn_tida"
        android:text="体达体温"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
      <Button
        android:id="@+id/btn_aiaoletemp"
        android:text="爱奥乐体温"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    </LinearLayout>
    <LinearLayout
      android:layout_width="wrap_content"
      android:layout_height="wrap_content">
      <Button
        android:id="@+id/btn_aike_sugar"
        android:text="艾科血糖"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
      <Button
        android:id="@+id/btn_aiaole_sugar"
        android:text="爱奥乐血糖"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    </LinearLayout>
    <Button
      android:id="@+id/btn_kangtai_uri"
      android:text="康泰尿液"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"/>
  </LinearLayout>
         